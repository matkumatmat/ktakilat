<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:tag="layout/dialog_time_out_0" android:background="@drawable/bg_round_time_out" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginRight="20dp">
    <ImageView android:id="@+id/image_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="38dp" android:src="@mipmap/icon_overtime" android:layout_centerHorizontal="true"/>
    <TextView android:textSize="18sp" android:textColor="#000000" android:id="@+id/text_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="@string/liveness_timeout" android:layout_below="@+id/image_icon" android:layout_centerHorizontal="true"/>
    <View android:id="@+id/view_line1" android:background="#e0e0e0" android:layout_width="match_parent" android:layout_height="0.3dp" android:layout_marginTop="30dp" android:layout_below="@+id/text_title"/>
    <Button android:textSize="18sp" android:textColor="#00baf2" android:id="@+id/btn_dialog_recollect" android:background="@android:color/transparent" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/liveness_retry" android:layout_below="@+id/view_line1"/>
    <View android:id="@+id/view_line2" android:background="#e0e0e0" android:layout_width="match_parent" android:layout_height="0.3dp" android:layout_below="@+id/btn_dialog_recollect"/>
    <Button android:textSize="18sp" android:textColor="#666666" android:id="@+id/btn_dialog_return" android:background="@android:color/transparent" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/liveness_back" android:layout_below="@+id/view_line2"/>
</RelativeLayout>
